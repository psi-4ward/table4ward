{
  "name": "psi/table4ward",
  "description": "Provides a TinyMCE editor for each cell of the table content-element. Just doubleclick a cell to open the rich-text-editor.",
  "keywords": ["contao", "TinyMCE", "Table"],
  "type": "contao-module",
  "homepage": "https://github.com/psi-4ward/table4ward",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Christoph Wiechert",
      "email": "wio@psitrax.de",
      "homepage": "http://4wardmedia.de",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "wio@psitrax.de",
    "issues": "https://github.com/psi-4ward/table4ward/issues",
    "forum": "https://community.contao.org/de/forumdisplay.php?110-4ward",
    "irc": "irc://irc.freenode.org/contao.dev",
    "source": "https://github.com/psi-4ward/table4ward"
  },
  "require": {
    "php": ">=5.3",
    "contao/core": ">=3.3,<4"
  },
  "replace": {
    "contao-legacy/table4ward": "self.version"
  },
  "conflict": {
    "contao/core": "3.0.*"
  },
  "extra": {
    "contao": {
      "sources": {
        "": "system/modules/table4ward",
        "tinyTable4ward.php": "system/config/tinyTable4ward.php"
      }
    },
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  }
}
